# Server Configuration
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/vynix

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Client URL
CLIENT_URL=http://localhost:3000

# AI API Keys (optional - only needed for external providers)
OPENAI_API_KEY=your-openai-api-key
GOOGLE_AI_KEY=your-google-ai-key
GROQ_API_KEY=your-groq-api-key
OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_REFERER=https://vynix.app

# Free Mode Configuration
FREE_MODE=true
CACHE_DISABLED=false
CACHE_MAX_ITEMS=500
CACHE_TTL_MS=86400000
ALLOW_BYOK_IN_FREE_MODE=false
KEY_ENCRYPTION_KEY=change_me_strong_random_32_bytes

# Content Safety Configuration
CONTENT_SAFETY_ENABLED=true